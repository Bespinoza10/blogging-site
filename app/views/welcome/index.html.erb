          <h2 class="section-titles"><span style="font-weight: bold;">.02 </span>My Perspective</h2>
          <hr align="left">
          <div class="col-md-12 wow fadeInLeft">
            <% @posts.each do |post| %>
              <h3><a><%= link_to post.title, post, class: "welcome-links" %></a></h3>
              <p class="dates"> <%= post.created_at.strftime("%A, %b %d") %> </p>
            <% end %>
          </div>
        </div>
        <div class="col-md-5 col-md-offset-1 wow fadeInRight no-padding per-pro-padding">
